        "ts/Forms/Layouts/ParallelLayout.ts",
        "ts/Forms/Interfaces/IForm.ts",
        "ts/Forms/Interfaces/IInputForm.ts",
        "ts/Forms/Interfaces/IButtonForm.ts",
        "ts/Forms/TextInput.ts",
        "ts/Forms/BigTextInput.ts",
        "ts/Forms/Button.ts",
        "ts/Forms/InformationBox.ts",
        "ts/Clock/Clock.ts",
        "ts/Search/SearchSuggestions.ts",
        "ts/Search/Homepage.ts",
        "ts/Search/Search.ts",
        "ts/Menubar/Sidebar/Sidebar.ts",
